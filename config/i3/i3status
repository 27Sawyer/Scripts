}

order += "time"
order += "volume master"
order += "wireless _first_"

time {
  format = "%d %b %Y  %H:%M"
}

volume master {
  format        = "♪ %volume"
  format_muted  = "♪ %volume"
  device        = "default"
  mixer         = "Master"
  mixer_idx     = 0
}

wireless _first_ {
  format_up   = "  %essid"
  format_down = "  down"
}

